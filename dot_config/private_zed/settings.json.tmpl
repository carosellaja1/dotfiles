{
  "features": {
    "edit_prediction_provider": "copilot"
  },
  "context_servers": {
    "serena-context-server": {
      "source": "extension",
      "enabled": true,
      "settings": {
        "python_executable": "/opt/homebrew/bin/python3.11"
      }
    },
    "mcp-server-mui": {
      "source": "extension",
      "enabled": true,
      "settings": {
        "preferred_theme": "dark",
        "component_filter": [""]
      }
    },
    "mcp-server-linear": {
      "source": "extension",
      "enabled": true,
      "settings": {
        "linear_api_key": "{{ env "LINEAR_API_KEY" }}"
      }
    },
    "mcp-server-supabase": {
      "source": "extension",
      "enabled": true,
      "settings": {
        "supabase_access_token": "{{ env "SUPABASE_ACCESS_TOKEN" }}",
        "read_only": false
      }
    },
    "mcp-server-figma": {
      "source": "extension",
      "enabled": true,
      "settings": {
        "figma_api_key": "{{ env "FIGMA_API_KEY" }}"
      }
    },
    "browser-tools-context-server": {
      "source": "extension",
      "enabled": true,
      "settings": {
        "server_args": ["@agentdeskai/browser-tools-server@1.2.0"],
        "server_command": "npx",
        "host": "127.0.0.1",
        "port": 3025
      }
    },
    "mcp-server-brave-search": {
      "source": "extension",
      "enabled": true,
      "settings": {
        "brave_api_key": "{{ env "BRAVE_API_KEY" }}"
      }
    },
    "mcp-server-context7": {
      "source": "extension",
      "enabled": true,
      "settings": {
        "context7_api_key": "{{ env "CONTEXT7_API_KEY" }}"
      }
    }
  },
  "icon_theme": {
    "mode": "system",
    "light": "Warm Charmed Icons",
    "dark": "VSCode Icons for Zed (Dark Angular)"
  },
  "agent": {
    "play_sound_when_agent_done": true,
    "model_parameters": []
  },
  "terminal": {
    "toolbar": {
      "breadcrumbs": true
    },
    "copy_on_select": true,
    "blinking": "on"
  },
  "use_system_window_tabs": false,
  "preview_tabs": {
    "enable_preview_from_code_navigation": true,
    "enable_preview_from_file_finder": true
  },
  "tabs": {
    "file_icons": true,
    "git_status": true
  },
  "title_bar": {
    "show_menus": true,
    "show_branch_icon": true
  },
  "close_on_file_delete": true,
  "search": {
    "include_ignored": true
  },
  "use_smartcase_search": true,
  "languages": {
    "CSS": {
      "indent_guides": {
        "background_coloring": "indent_aware"
      }
    }
  },
  "diagnostics": {
    "inline": {
      "enabled": true
    }
  },
  "prettier": {
    "allowed": true
  },
  "hard_tabs": false,
  "toolbar": {
    "code_actions": true
  },
  "autosave": {
    "after_delay": {
      "milliseconds": 1000
    }
  },
  "linked_edits": true,
  "base_keymap": "VSCode",
  "ui_font_size": 16,
  "buffer_font_size": 15,
  "theme": {
    "mode": "system",
    "light": "Catppuccin Espresso (Blur)",
    "dark": "Zedokai Darker (Filter Octagon)"
  }
}
